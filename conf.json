            "RecordsAbstract": "computers",
            "UsersAbstract": "users",
            "AssetID": "Asset.Name"
            
        },
        {
            "AssetType":"Laptop",
            "Filter":"Status eq 'Active' and Manufacturer eq 'Dell Inc.' and indexof(Model, 'Latitude') ge 0",
            "Description":"Active Laptops",
            "RecordsAbstract": "computers",
            "UsersAbstract": "users",
            "AssetID": "Asset.Name"
        }
